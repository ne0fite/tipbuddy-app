{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
        "prefer-destructuring": 0,
        "jsx-a11y/": 0,
        "react/destructuring-assignment": 0,
        "react/jsx-filename-extension": 0,
        "react/prefer-stateless-function": 0,
        "react/prop-types": 0,
        "comma-dangle": "off",
        "no-param-reassign": ["error", { "props": false }]
    },
    "settings": {
    },
    "env": {
        "es6": true
    }
  }
