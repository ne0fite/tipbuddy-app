{
  "expo": {
    "name": "Tip Stash",
    "slug": "tipstash-app",
    "description": "Track your tip earnings",
    "githubUrl": "https://github.com/ne0fite/tipstash-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#990000",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phoenixbright.tipstash",
      "usesIcloudStorage": true
    },
		"android": {
			"package": "com.phoenixbright.tipstash"
    }
  }
}
